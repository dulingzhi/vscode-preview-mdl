{
  "name": "vscode-preview-mdl",
  "displayName": "Preview MDL file",
  "description": "Preview Warcraft III mdl.",
  "version": "0.1.0",
  "publisher": "Jai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dulingzhi/vscode-preview-mdl"
  },
  "bugs": {
    "url": "https://github.com/dulingzhi/vscode-preview-mdl/issues"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "MDL",
    "Preview",
    "VSCode",
    "multi-root ready",
    "Warcraft III"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:extension.preview",
    "onCommand:extension.launch",
    "onCommand:extension.stop",
    "onCommand:extension.resume",
    "onCommand:extension.ui"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "PreviewServer",
      "properties": {
        "previewServer.browsers": {
          "type": "array",
          "default": null,
          "description": "%previewServer.browsers.description%",
          "items": {
            "type": "string",
            "enum": [
              "firefox",
              "chrome",
              "opera",
              "safari"
            ]
          }
        },
        "previewServer.ignoreDefaultBrowser": {
          "type": "boolean",
          "default": false,
          "description": "%previewServer.ignoreDefaultBrowser.description%"
        },
        "previewServer.ignoreNotification": {
          "type": "boolean",
          "default": false,
          "description": "%previewServer.ignoreNotification.description%"
        },
        "previewServer.isWatchConfiguration": {
          "type": "boolean",
          "default": true,
          "description": "%previewServer.isWatchConfiguration.description%"
        },
        "previewServer.port": {
          "type": [
            "number",
            "null"
          ],
          "default": 8080,
          "description": "%previewServer.port.description%"
        },
        "previewServer.proxy": {
          "type": "string",
          "default": "",
          "description": "%previewServer.proxy.description%"
        },
        "previewServer.startupProject": {
          "type": "string",
          "default": null,
          "description": "%previewServer.startupProject.description%"
        },
        "previewServer.sync": {
          "type": "boolean",
          "default": true,
          "description": "%previewServer.sync.description%"
        },
        "previewServer.ui": {
          "type": "object",
          "description": "%previewServer.ui.description%",
          "properties": {
            "port": {
              "type": "number",
              "default": 3001,
              "description": "Port number of UI."
            },
            "weinrePort": {
              "type": "number",
              "default": 8081,
              "description": "Port number of weinre."
            }
          }
        }
      }
    },
    "commands": [
      {
        "command": "extension.preview",
        "title": "%extension.preview.title%"
      },
      {
        "command": "extension.launch",
        "title": "%extension.launch.title%"
      },
      {
        "command": "extension.stop",
        "title": "%extension.stop.title%"
      },
      {
        "command": "extension.resume",
        "title": "%extension.resume.title%"
      },
      {
        "command": "extension.ui",
        "title": "%extension.ui.title%"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.preview",
          "when": "resourceExtname == .mdl"
        },
        {
          "command": "extension.launch",
          "when": "resourceExtname == .mdl"
        }
      ],
      "editor/context": [
        {
          "command": "extension.preview",
          "when": "resourceExtname == .mdl"
        },
        {
          "command": "extension.launch",
          "when": "resourceExtname == .mdl"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/browser-sync": "0.0.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^7.0.5",
    "@types/opener": "^1.4.0",
    "del": "^3.0.0",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.1.0",
    "gulp-typescript": "^3.2.3",
    "mocha": "^2.3.3",
    "run-sequence": "^2.2.0",
    "tslint": "^4.5.1",
    "typescript": "^2.6.2",
    "vsce": "^1.35.0",
    "vscode": "^1.1.6",
    "vscode-nls-dev": "^2.1.6"
  },
  "dependencies": {
    "browser-sync": "^2.24.4",
    "opener": "^1.4.3",
    "pngjs": "^3.3.3",
    "vscode-nls": "^2.0.2",
    "war3-model": "^1.0.1"
  }
}
